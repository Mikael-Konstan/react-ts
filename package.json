    "axios": "^0.27.2",
    "i18next": "^21.8.14",
    "js-cookie": "^3.0.1",
    "mockjs": "^1.1.0",
    "react": "17.x",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "17.x",
    "react-i18next": "^11.18.1",
    "react-infinite-scroll-component": "^6.1.0",
    "umi": "^3.5.26"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
